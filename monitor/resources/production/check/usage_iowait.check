  - name: IOWait Max
    detail: Minimum IOWait has exceeded a 25% CPU threshold for over 5 mins
    calendar: OfficeHours
    fluxfile: flux/usage_iowait.flux
    severity: 1
    threshold: 25 
    comparison: gt
    tags: 
      - host
      - _field
      - _measurement
    interval: 300
    target: postfix
